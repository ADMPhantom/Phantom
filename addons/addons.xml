<addons>
<addon id="plugin.video.phwizard" version="1.0.0" name="Config Wizard" provider-name="Phantom">
<requires>
<import addon="xbmc.python" version="2.1.0"/>
<import addon="script.module.addon.common" optional="true"/>
<import addon="script.module.t0mm0.common" optional="true"/>
<import addon="script.module.urlresolver" optional="true"/>
</requires>
<extension point="xbmc.python.pluginsource" library="default.py">
<provides>executable</provides>
</extension>
<extension point="xbmc.addon.metadata">
<summary>Config Wizard</summary>
<description>
Instantly configure your Kodi experience with all the best add-ons and goodies from the Phantom TV!
</description>
<platform>all</platform>
<forum></forum>
<website></website>
</extension>
</addon>
